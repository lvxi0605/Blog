<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="edu.jxufe.lvxi.blog.core.persist.entity.system.CityEntity" table="tb_sys_city" schema="blog">
        <id name="id">
            <column name="ID" sql-type="int(10) unsigned"/>
        </id>
        <property name="code">
            <column name="code" sql-type="int(10) unsigned"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="provinceCode">
            <column name="provinceCode" sql-type="int(10) unsigned"/>
        </property>
    </class>
</hibernate-mapping>